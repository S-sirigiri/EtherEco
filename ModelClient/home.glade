<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkWindow" id="window">
    <property name="title" translatable="yes">EtherEco</property>
    <property name="default_width">300</property>
    <property name="default_height">200</property>
    <child>
      <object class="GtkBox" id="box">
        <property name="orientation">vertical</property>
        <property name="spacing">5</property>
        <child>
          <object class="GtkLabel" id="welcome_label">
            <property name="label" translatable="yes">Welcome to EtherEco</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="signin_button">
            <property name="label" translatable="yes">Sign In</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="signup_button">
            <property name="label" translatable="yes">Sign Up</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
